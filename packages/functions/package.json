{
  "name": "@travellers-api/functions",
  "version": "0.0.0",
  "private": true,
  "engines": {
    "node": "20"
  },
  "main": "lib/index.js",
  "scripts": {
    "build": "rimraf lib && tsc",
    "build:watch": "rimraf lib && tsc --watch",
    "serve": "pnpm run build && firebase emulators:start --only functions",
